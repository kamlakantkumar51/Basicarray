#include<iostream>
using namespace std;
void printArray(int arr[],int size){
    cout<<"printing the array....."<<endl;
    for(int i=1;i<size;i++){
        cout<<arr[i]<<" ";
    }
    cout <<"printing done " <<endl;
}
int main(){
    int number[15];//declaration
    cout << "value at 14 index is :"<< number[14] << endl;//accesiing an array

    int second[3]={5,7,11};//iniatilization of an array
    cout << "value at 2 index is :"<<second[2] << endl;//accessing

    int third[15]={2,7};
    int n=15;
    printArray(third,15);
    int thirdSize= sizeof(third)/sizeof(int);
    cout << "Size of third is: " <<thirdSize <<endl;



}
